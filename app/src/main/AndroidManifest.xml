<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.notes.electronicjournal">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon_app"
        android:label="@string/app_name"
        android:roundIcon="@drawable/mainpic"
        android:supportsRtl="true"
        android:theme="@style/Theme.ElectronicJournal">
        <activity android:name=".EditRecordActivity"></activity>
        <activity android:name=".PersonsJournalActivity" />
        <activity android:name=".AddRecordActivity" />
        <activity android:name=".JournalActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>